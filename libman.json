{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "globalize@1.7.0",
      "destination": "wwwroot/globalize/"
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/"
    },
    {
      "provider": "jsdelivr",
      "library": "zoomify@0.2.5",
      "destination": "wwwroot/lib/zoomify/",
      "files": [
        "src/zoomify.css",
        "src/zoomify.js",
        "src/zoomify.min.css",
        "src/zoomify.min.js"
      ]
    }
  ]
}